.container {
  display: flex;
  flex-direction: column;
  align-items: center;
}
.kb {
  width: 750rpx;
  height: 100rpx;
  background-color: #f8f6f6(243, 240, 245);
}
.bocc {
  width: 750rpx;
  height: 210rpx;
  position: relative;
  background-color: rgb(248, 245, 242);
}
.M {
  width: 100%;
  height: 60rpx;
  text-align: center;
  background-color: #f8f6f6;
}
.user-avatar {
  position: absolute;
  left: 50%;
  top: -10%;
  margin-left: -60px;
  margin-top: -10px;
  height: 120px;
  width: 120px;
}
.item {
  width: 200rpx;
  height: 200rpx;
  background-color: #f8f6f6(238, 241, 234);
  text-align: center;
  padding-top: 20px;
}
page {
  background-color: #f8f6f6;
}
view {
  box-sizing: border-box;
}
.user-box {
  width: 99%;
  overflow: hidden;
  background-color: #fff;
  margin: 0 auto;
}
.user-box .ctn .list {
  text-align: center;
  width: 33%;
  padding: 20rpx 0 20rpx 0;
  display: inline-block;
}
